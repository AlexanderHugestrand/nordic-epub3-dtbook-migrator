                <dtbook:book/>
            </dtbook:dtbook>
        </x:context>
        <x:expect label="should be allowed inside the dtbook element">
            <html:html epub:prefix="z3998: http://www.daisy.org/z3998/2012/vocab/structure/#">
                <html:head>...</html:head>
                <html:body id="...">...</html:body>
            </html:html>
        </x:expect>
    </x:scenario>

